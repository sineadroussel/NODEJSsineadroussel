generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

//cr√©er leurs noms, metiers et age//
model agent {
id    Int   @id @default(autoincrement())
name  String 
metier String @default("etudiants")
age   Int  @default(20)
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt

}    